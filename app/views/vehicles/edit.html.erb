<h5>Create new vehicle</h5>

<%= form_for(@vehicle, class: "col s12 m6 offset-m3") do |f| %>
  <input type="hidden" name="_method" value="patch">
  <%= render 'vehicles/partials/vehicle_form', f: f %>

  <div class="row">
    <%= submit_tag "Update", class: 'btn col s12 m6 offset-m3' %>
  </div>

<% end %>
