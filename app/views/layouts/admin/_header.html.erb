<div id='header-container'>
  <div id='header-container-inner'>
    <div id='header-inner'>
      <%= link_to "/" do %>
        <%= image_tag "BigTreeServicesLogo.png", :id => 'header-logo', :alt => 'Big Tree Services Logo' %>
      <% end %>
      <br>
      <div id="header__subtitle">Admin</div>

      <span class = "menu-icon" onclick="toggleMenu()"><i class="fa fa-bars"></i></li>
    </div>

    <div id="header__nav-container">
      <%= render 'layouts/admin/nav' %>
    </div>
  </div>
</div>
