<section id="admin-container">
	<div id="admin-nav-bar">		
		<div id="admin-nav-element" class="form__submit">
			<%= link_to '/receipts/new', class: "form__submit-link" do %>
				Create Receipt
			<% end %>
		</div>
			<div id="admin-nav-element" class="form__submit">
				<%= link_to '/receipts', class: "form__submit-link" do %>
					My Receipts
				<% end %>
			</div>
	</div>

	<div id="admin-nav-bar">		
		<div id="admin-nav-element" class="form__submit">
			<%= link_to '/vehicles', class: "form__submit-link" do %>
				Vehicles
			<% end %>
		</div>
	</div>


	<h3>Running work</h3>

	<div id ="admin__estimate-table">
		<% @estimates.each do |estimate| %>
			<%= render "single_estimate_entry", estimate: estimate %>
		<% end %>
	</div>


	<%= render "arborists/arborist_table", arborists: @arborists %>
</section>
